{{ header }}{{ column_left }}
<div id="content">
  <div class="page-header">
    <div class="container-fluid">
     <div class="breadcrumb">
       {% for breadcrumb in breadcrumbs %}
       {{ breadcrumb.separator }}<a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a>
       {% endfor %}
     </div>
     <div class="container" style="width: auto !important;">
     {% if error_warning %}
     <div class="warning">{{ error_warning }}</div>
     {% endif %}
     {% if success %}
     <div class="success">{{ success }}</div>
     {% endif %}
     <div class="box">
      <div class="heading">
        <h1><img src="view/image/backup.png" alt="" /> {{ heading_base_title }}</h1>	    
      </div>
	  <div class="text-right">
		<a onclick="$('#form').submit();" data-toggle="tooltip" class="btn btn-primary" type="button"><i>Start / Continue</i></a>
	  </div>
      <div class="panel panel-default">
     <div class="panel-body">
      <div class="content">
        <form action="{{ start }}" method="post" enctype="multipart/form-data" id="form">
          <table class="form">
           <tr>
            <td colspan="2">{{ entry_description }}</td>
           </tr>
           <tr>
            <td width="25%">{{ entry_restore }}</td>
            <td><input type="file" name="xmlfile" /></td>
		   </tr>
          </table>
       </form>
       </div>
       </div>
      </div>
     </div>
    </div>
  </div>
</div>
{{ footer }}